# Source directory containing certificates to assign
CERTS_DIR=/path/to/certificates/source

# Directory where assigned certificates will be saved
ASSIGNED_CERTS_DIR=/path/to/certificates/assigned

# HTTP Basic Authentication credentials
AUTH_USERNAME=your_username
AUTH_PASSWORD=your_secure_password

# Flask configuration
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
FLASK_DEBUG=False
